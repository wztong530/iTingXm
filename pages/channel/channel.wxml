<!-- 佣金说明弹窗 -->
<view wx:if="{{isShowCommission}}" class='commission_shows'>
  <view class='mask'>
  <view class='shows'>
    <view class='title'>佣金说明</view>
    <view class='text'>
      <view wx:for='{{notice}}'>{{index +1}}. {{item.text}}</view>
    </view>
    <view class='shows_btn' catchtap='hideCommission'>知道了</view>
  </view></view>  
</view>