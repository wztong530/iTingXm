.content {
  padding-top: 22rpx;
}

.list {
  width: 710rpx;
  background-color: #fff;
  margin: 0 auto;
  box-shadow: 0 13px 11px 0 rgba(0, 0, 0, 0.06);
  border-radius: 2px;
  box-sizing: border-box;
  padding: 0 30rpx;
  margin-bottom: 110rpx;
}

.list .title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 80rpx;
}

.list .title text {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #4a4a4a;
  letter-spacing: 0;
}



.launch {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-image: var(--linearTwo);
}

.launch text {
  font-family: PingFangSC-Medium;
  font-size: 16px;
  color: #fff;
  letter-spacing: 0;
  line-height: 17px;
}

.hasdata {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100rpx;
  border-top: 2rpx solid rgba(233, 233, 233, 0.6);
}

.hasdata text {
  opacity: 0.3;
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #222333;
  letter-spacing: 0;
  text-align: center;
  line-height: 12px;
}


.list-item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 0;
  border-top: 2rpx solid rgba(233, 233, 233, 0.6);
}

.list-item image {
  flex-shrink: 0;
  width: 96rpx;
  height: 96rpx;
  border-radius: 48rpx;
}

.list-item .user {
  position: relative;
  flex-shrink: 0;
  width: 160rpx;
}

.list-item .user .my {
  position: absolute;
  top: -16rpx;
  background: var(--text);
  border-radius: 1px;
  width: 62rpx;
  height: 28rpx;
  text-align: center;
  line-height: 28rpx;
  font-family: PingFangSC-Regular;
  font-size: 10px;
  color: #fff;
  letter-spacing: 0.29px;
}

.list-item .user .name {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #000;
  letter-spacing: 0.4px;
}

.list-item .info {
  display: flex;
  flex-shrink: 0;
  flex-direction: column;
  align-items: flex-end;
  justify-content: center;
}

.list-item .info text {
  color: #4a4a4a;
}

.list-item .info .num {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  letter-spacing: 0.34px;
}

.list-item .info .time {
  font-family: PingFangSC-Light;
  font-size: 11px;
  letter-spacing: 0.31px;
}

.list-item .btn {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  flex-shrink: 0;
  width: 156rpx;
  height: 64rpx;
  background-image: var(--linearTwo);
  border-radius: 200rpx;
}
.list-item .btn.fail{
  background: #D0D0D0;
}
.list-item .btn text {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: #fff;
  letter-spacing: 0.34px;
  line-height: 10px;
}

.list-item .btn .price {
  letter-spacing: 0.29px;
  line-height: 12px;
}

.list-item .btn .mark {
  font-size: 8px;
}

.list-item .btn .tip {
  font-size: 10px;
  letter-spacing: 0.29px;
}
.txtcolor{
  color:var(--text)!important;
}